<vertical vertical-fit="PreferredSize" spacing="2" child-align="LowerCenter" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd">
  <horizontal bg ="round-rect-panel">
    <button text="Undo" pref-width="35" on-click="undo" />
    <button text="Redo" pref-width="35" on-click="redo" />
  </horizontal>
  <horizontal bg ="round-rect-panel">
    <button text="Cancel" on-click="exit-edit-mode" pref-width="25" />
    <button text="Reset" on-click="reset" pref-width="25" />
    <button text="Apply" on-click="apply" pref-width="25" />
  </horizontal>
</vertical>